<?xml version="1.0" encoding="UTF-8" ?>
<CycloneDX xmlns="https://github.com/eclipse-cyclonedx/cyclonedx"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="https://github.com/eclipse-cyclonedx/cyclonedx
           https://github.com/eclipse-cyclonedx/cyclonedx/Domain/Discovery.xml">
    <Domain id="any">
        <General>
            <NetworkInterfaceAddress>auto</NetworkInterfaceAddress>
            <AllowMulticast>true</AllowMulticast>
            <MaxMessageSize>65000</MaxMessageSize>
        </General>
        <Scheduling>
            <WorkerThreads>auto</WorkerThreads>
        </Scheduling>
    </Domain>
</CycloneDX>